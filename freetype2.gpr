library project FreeType2 is

  for Languages use ("ada");
  for Library_Name use "freetype2.ada";
  for Source_Dirs use ("src");
  for Object_Dir use "obj";
  for Library_Dir use "lib";
  for Library_Kind use "static";
  
	package Linker is
		for Linker_Options use ("-Llib",  "-lfreetype");
	end Linker;
	
end FreeType2;